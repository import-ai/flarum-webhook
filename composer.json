{
    "name": "import-ai/flarum-webhook",
    "description": "Trigger webhooks when users create new posts",
    "keywords": [
        "flarum"
    ],
    "type": "flarum-extension",
    "license": "Apache-2.0",
    "support": {
        "source": "https://github.com/import-ai/flarum-webhook"
    },
    "require": {
        "flarum/core": "^1.2.0"
    },
    "authors": [
        {
            "name": "Import AI",
            "homepage": "https://github.com/import-ai",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "ImportAI\\Webhook\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "Post Webhook",
            "category": "feature",
            "icon": {
                "name": "fas fa-paper-plane",
                "color": "#fff",
                "backgroundColor": "#4a90d9"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
